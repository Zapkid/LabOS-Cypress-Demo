{
  "name": "labos-cypress-demo",
  "version": "1.0.0",
  "description": "Automated test cases for the LabOS website using Cypress automation and the page-object-model design pattern.",
  "main": "index.js",
  "scripts": {
    "run-tests:all": "npx cypress run --browser chrome --spec 'cypress/e2e/LabOS/*.cy.ts'",
    "run-tests:login": "npx cypress run --browser chrome --spec 'cypress/e2e/LabOS/login.cy.ts'",
    "run-tests:order-success": "npx cypress run --browser chrome --spec 'cypress/e2e/LabOS/order-success.cy.ts'",
    "run-tests:order-fail": "npx cypress run --browser chrome --spec 'cypress/e2e/LabOS/order-fail.cy.ts'",
    "open-cypress": "npx cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zapkid/LabOS-Cypress-Demo.git"
  },
  "keywords": [
    "automation",
    "testing",
    "cypress"
  ],
  "author": "Rowan Kendal",
  "license": "none",
  "bugs": {
    "url": "https://github.com/Zapkid/LabOS-Cypress-Demo/issues"
  },
  "homepage": "https://github.com/Zapkid/LabOS-Cypress-Demo#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "cypress": "^13.6.2",
    "cypress-real-events": "^1.11.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  }
}
